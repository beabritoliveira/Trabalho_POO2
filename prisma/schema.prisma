model Recem_Nascido {
  id    Int     @id @default(autoincrement())
  nome  String
  data_nascimento DateTime
  altura Float
  peso Float
  mae Mae @relation(fields: [maeId], references: [id])
  maeId Int
}

model Mae {
  id        Int     @id @default(autoincrement())
  nome     String
  endereco   String?
  telefone String?
  data_nascimento DateTime
  bebe Recem_Nascido[]
  parto Parto[]
}

model Medico {
  CRM  Int @id 
  nome String
  telefone String?
  especializada String
  parto Parto[]
}

model Parto {
  idParto   Int  @id @default(autoincrement())
  data DateTime
  medico Medico @relation(fields: [medID], references: [CRM])
  medID Int
  paciente Mae @relation(fields: [pacID], references: [id])
  pacID Int

}